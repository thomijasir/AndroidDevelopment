<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thomionewave.animalcustomearray">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Hewan Ternak" android:name=".Ternak" />
        <activity android:label="Hewan Hutan" android:name=".Hutan" />
        <activity android:label="Hewan Laut" android:name=".Laut" />
        <activity android:label="Hewan Langka" android:name=".Langka">
        </activity>
    </application>

</manifest>